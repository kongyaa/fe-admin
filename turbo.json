{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "TURBO_TOKEN",
    "TURBO_TEAM"
  ],
  "remoteCache": {
    "enabled": true
  },
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
