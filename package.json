{
  "name": "cache-example",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "clean": "turbo clean && rm -rf node_modules",
    "build:remote": "env-cmd turbo build --cache-dir='.turbo' --cache=remote:rw",
    "dev:remote": "env-cmd turbo dev --cache-dir='.turbo' --cache=remote:rw"
  },
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "marked": "^15.0.7",
    "openai": "^4.85.0",
    "turbo": "^2.4.4"
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "dotenv-cli": "^7.4.4",
    "env-cmd": "^10.1.0",
    "prettier": "^3.5.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
